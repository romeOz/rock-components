{
    "name": "romeoz/rock-components",
    "description": "[READ ONLY] Components module for Rock Framework",
    "keywords": ["components", "behaviors", "filters"],
    "type": "library",
    "license": "MIT",
    "support": {
        "email": "serggalka@gmail.com",
        "issues": "https://github.com/romeOz/rock-components/issues",
        "source": "https://github.com/romeOz/rock-components"
    },
    "authors": [
        {
            "name": "Ruslan",
            "email": "serggalka@gmail.com"
        }
    ],
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/romeOz/rock-base"
    }],
    "require": {
        "php": ">=5.4.0",
        "romeoz/rock-base" : "*@dev",
        "romeoz/rock-helpers" : "*@dev",
        "romeoz/rock-events" : "*@dev"
    },
    "require-dev": {
        "romeoz/rock-sanitize" : "*@dev",
        "romeoz/rock-validate" : "*@dev",
        "phpunit/phpunit": "4.4.*"
    },
    "autoload": {
        "psr-4": {
            "rock\\components\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}